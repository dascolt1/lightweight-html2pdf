  "author": "Thomas D'Ascoli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dascolt1/lightweight-html2pdf.git"
  },
  "bugs": {
    "url": "https://github.com/dascolt1/lightweight-html2pdf/issues"
  },
  "homepage": "https://github.com/dascolt1/lightweight-html2pdf#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^30.1.1",
    "jsdom": "^26.1.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
